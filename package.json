{
  "name": "@aller/google-cloud-secret",
  "description": "Concurrent update of google cloud secret",
  "version": "0.1.1",
  "type": "module",
  "main": "./lib/index.cjs",
  "module": "./src/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "test": "mocha",
    "posttest": "npm run lint && npm run build",
    "lint": "eslint . --cache && prettier . -c",
    "cov:html": "c8 -n src -r html -r text mocha",
    "test:lcov": "c8 -n src -r lcov -r text mocha",
    "build": "rollup -c && dts-buddy",
    "prepublishOnly": "npm run build"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "require": "./lib/index.cjs",
      "import": "./src/index.js"
    },
    "./fake-server/fake-secret-manager-server": {
      "types": "./types/index.d.ts",
      "require": "./lib/fake-server/fake-secret-manager-server.cjs",
      "import": "./src/fake-server/fake-secret-manager-server.js"
    }
  },
  "engines": {
    "node": ">=22"
  },
  "keywords": [
    "google cloud secret manager",
    "secret",
    "cache",
    "concurrent",
    "update"
  ],
  "author": {
    "name": "Aller Media",
    "url": "https://github.com/allermedia"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allermedia/google-cloud-secret.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@google-cloud/secret-manager": "^6.1.0",
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.8.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@types/debug": "^4.1.12",
    "c8": "^10.1.3",
    "chai": "^6.0.1",
    "chronokinesis": "^7.0.0",
    "debug": "^4.4.1",
    "dts-buddy": "^0.6.2",
    "eslint": "^9.33.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsonc": "^2.20.1",
    "google-auth-library": "10.3.1",
    "lru-cache": "^11.2.1",
    "mocha": "^11.7.1",
    "mocha-cakes-2": "^3.3.0",
    "nock": "^14.0.9",
    "prettier": "^3.6.2",
    "rollup": "^4.46.3"
  },
  "peerDependencies": {
    "@google-cloud/secret-manager": "6.x",
    "@grpc/grpc-js": "1.x",
    "@grpc/proto-loader": "0.x",
    "debug": ">=4",
    "long": ">=5",
    "lru-cache": ">=11"
  },
  "files": [
    "lib",
    "src",
    "types/index.d*"
  ],
  "c8": {
    "exclude": [
      "src/fake-server"
    ]
  }
}
